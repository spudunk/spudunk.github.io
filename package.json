{
  "name": "spudunk.github.io",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "npm-run-all compile:sass copy:public",
    "watch:sass": "node-sass scss/*.scss public/style.css -w",
    "compile:sass": "node-sass --output-style compressed scss/*.scss public/style.css",
    "copy:public": "cp -r public/* ../spudunk.github.io/",
    "dev-server": "live-server public",
    "start": "npm-run-all --parallel dev-server watch:sass"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spudunk/spudunk.github.io.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/spudunk/spudunk.github.io/issues"
  },
  "homepage": "https://github.com/spudunk/spudunk.github.io#readme",
  "devDependencies": {
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5"
  }
}
